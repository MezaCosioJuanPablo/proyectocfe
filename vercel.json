{
  "version": 2,
  "routes": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index.js"
    }
  ],
  "rewrites": [
    {
      "source": "/custom-path",
      "destination": "/original-path"
    }
  ],
  "redirects": [
    {
      "source": "/old-route",
      "destination": "/new-route",
      "statusCode": 301
    }
  ]
}
